{
  "name": "moxy-proxy",
  "version": "0.99.9",
  "description": "Proxy server for mocking client application",
  "main": "server/index.ts",
  "repository": "git@github.com:G3F4/moxy-proxy.git",
  "homepage": ".",
  "author": "G3F4 <kamil.grabek@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn run:server",
    "start:local:with-build": "yarn build:staging;yarn run:server",
    "run:server": "node dist/server",
    "host:client": "http-server -c-1 dist/public",
    "test": "yarn test:bdd:local:headless:with-app",
    "test:bdd:local:chrome": "env-cmd -f .env.staging gherkin-testcafe chrome __bddTests__/**/*.steps.ts __bddTests__/**/*.feature --skip-js-errors;yarn test:bdd:cleanup",
    "test:bdd:local:chrome:with-app": "env-cmd -f .env.staging npm-run-all -p -r test:bdd:app test:bdd:local:chrome;yarn test:bdd:cleanup",
    "test:bdd:local:headless": "env-cmd -f .env.staging gherkin-testcafe chrome:headless __bddTests__/**/*.steps.ts __bddTests__/**/*.feature --skip-js-errors;yarn test:bdd:cleanup",
    "test:bdd:local:headless:with-app": "env-cmd -f .env.staging npm-run-all -p -r test:bdd:app test:bdd:local:headless;yarn test:bdd:cleanup",
    "test:bdd:app": "env-cmd -f .env.staging npm-run-all -p -r run:server host:client",
    "test:bdd:cleanup": "rm -rf __testsData__",
    "test:client": "react-scripts test",
    "build": "yarn build:clean;npm-run-all -p build:client build:server",
    "build:staging": "yarn build:clean;npm-run-all -p build:client:staging build:server",
    "build:client": "rm -rf dist/public;react-scripts build;mv build dist/public",
    "build:client:staging": "env-cmd -f .env.staging npm run build:client",
    "build:server": "tsc -p tsconfig.server.prod.json",
    "build:clean": "rm -rf dist;mkdirp dist",
    "dev": "npm-run-all -p -r dev:server dev:client",
    "dev:server": "env-cmd -f .env.dev ts-node-dev --project tsconfig.server.dev.json --watch server --respawn --transpileOnly server/index.ts",
    "dev:client": "PORT=8888 react-scripts start",
    "lint": "eslint './**/*.{ts,tsx}'",
    "lint:fix": "eslint './**/*.{ts,tsx}' --fix",
    "client:deploy:predeploy": "yarn build:client",
    "client:deploy:gh-pages": "gh-pages -d dist/public"
  },
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@types/mkdirp": "1.0.1",
    "@types/node": "14.11.2",
    "@types/parseurl": "1.3.1",
    "@types/rimraf": "3.0.0",
    "dotenv": "8.2.0",
    "fastify": "3.4.1",
    "fastify-cors": "4.1.0",
    "fastify-websocket": "2.0.7",
    "immer": "7.0.9",
    "parseurl": "1.3.3",
    "quicktype-core": "6.0.69",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@monaco-editor/react": "3.6.3",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "12.1.6",
    "@types/jest": "26.0.14",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-syntax-highlighter": "11.0.5",
    "cucumber": "5.1.0",
    "env-cmd": "10.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-testcafe": "0.2.1",
    "gh-pages": "3.1.0",
    "gherkin-testcafe": "2.5.1",
    "http-server": "0.12.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-json-view": "1.19.1",
    "react-monaco-editor": "0.40.0",
    "react-prism": "4.3.2",
    "react-scripts": "3.4.3",
    "react-syntax-highlighter": "13.5.3",
    "testcafe": "1.9.3",
    "ts-jest": "26.4.0",
    "ts-node-dev": "1.0.0-pre.44"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
