{
  "name": "moxy-proxy",
  "version": "0.88.3",
  "description": "Proxy server for mocking client application",
  "main": "server/index.ts",
  "repository": "git@github.com:G3F4/moxy-proxy.git",
  "homepage": "https://g3f4.github.io/moxy-proxy/",
  "author": "G3F4 <kamil.grabek@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node --project tsconfig.server.json server/index.ts",
    "test": "NODE_ENV=test yarn run test:e2e",
    "test:e2e": "yarn test:data:init && NODE_ENV=test testcafe chrome:headless __tests__/*.test.* --skip-js-errors --app 'yarn start' --app-init-delay 10000; yarn test:data:cleanup",
    "test:e2e:local": "yarn test:data:init && testcafe chrome __tests__/*.test.* --skip-js-errors --app 'NODE_ENV=test yarn start'  --app-init-delay 10000; yarn test:data:cleanup",
    "test:e2e:local:single": "yarn test:data:init && NODE_ENV=test testcafe chrome __tests__/serverState.test.ts --skip-js-errors --app 'NODE_ENV=test yarn start'  --app-init-delay 10000; yarn test:data:cleanup",
    "test:e2e:remote": "APP_URL=https://moxy-proxy.herokuapp.com/ testcafe chrome __tests__/e2e.test.* --skip-js-errors",
    "test:client": "react-scripts test",
    "test:data:init": "yarn test:data:cleanup && mkdirp __testsData__/data && cp -a __testsData__/initial/* __testsData__/data",
    "test:data:cleanup": "rm -rf __testsData__/data",
    "build:client": "react-scripts build",
    "dev:server": "ts-node-dev --project tsconfig.server.json --ignore-watch mockedData --respawn --transpileOnly server/index.ts",
    "dev:client": "PORT=8888 react-scripts start",
    "lint": "eslint './**/*.{ts,tsx}'",
    "lint:fix": "eslint './**/*.{ts,tsx}' --fix",
    "predeploy": "yarn build:client",
    "deploy": "gh-pages -d build"
  },
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@types/mkdirp": "0.5.2",
    "@types/node": "13.1.6",
    "@types/parseurl": "1.3.1",
    "@types/rimraf": "2.0.3",
    "dotenv": "8.2.0",
    "fastify": "2.11.0",
    "fastify-cors": "3.0.1",
    "fastify-websocket": "1.1.0",
    "immer": "5.2.1",
    "maketypes": "1.1.2",
    "parseurl": "1.3.3",
    "ts-node": "8.6.1",
    "typescript": "3.7.4"
  },
  "devDependencies": {
    "@material-ui/core": "4.9.0",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.40",
    "@monaco-editor/react": "3.0.1",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/jest": "25.1.3",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "@types/react-syntax-highlighter": "11.0.4",
    "@types/supertest": "2.0.8",
    "eslint-plugin-testcafe": "0.2.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "gh-pages": "3.1.0",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-json-view": "1.19.1",
    "react-monaco-editor": "0.33.0",
    "react-prism": "4.3.2",
    "react-scripts": "3.3.0",
    "react-syntax-highlighter": "12.2.1",
    "supertest": "4.0.2",
    "testcafe": "1.8.2",
    "ts-jest": "25.2.1",
    "ts-node-dev": "1.0.0-pre.44"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
